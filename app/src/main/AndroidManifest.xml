<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.main_fitness_app">

    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".stepcounter"/>
        <activity android:name=".finishmeal"/>
        <activity android:name=".WorkOut" />
        <activity android:name=".WorkOutFull" />
        <activity android:name=".ResetProfile" />
        <activity android:name=".ShowProfile" />
        <activity android:name=".BodyCheck" />
        <activity android:name=".NextRegister" />
        <activity android:name=".Register" />
        <activity android:name=".foodmain" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>